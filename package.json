{
  "name": "lopenling-app",
  "author": "Tenzin Kunsang",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css && remix build ",
    "build:css": "npm run generate:css -- --minify ",
    "deploy": "wrangler publish",
    "dev": "remix build && run-p \"dev:*\" ",
    "dev:remix": "remix watch",
    "dev:miniflare": "cross-env NODE_ENV=development miniflare ./build/index.js --watch --kv SESSIONS --kv-persist --compat-flag streams_enable_constructors",
    "dev:css": "npm run generate:css -- --watch",
    "generate:css": "tailwindcss -o ./app/styles/tailwind.css",
    "start": "cross-env NODE_ENV=production miniflare ./build/index.js",
    "test": "npx cypress run",
    "cypress-open": "npx cypress open",
    "pretty": "prettier --write \"./**/*.{js,jsx,tsx,json}\""
  },
  "dependencies": {
    "@assembless/react-littera": "^2.4.0",
    "@hocuspocus/provider": "^2.0.3",
    "@hocuspocus/transformer": "^2.0.3",
    "@prisma/client": "^4.12.0",
    "@remix-run/cloudflare": "^1.11.1",
    "@remix-run/cloudflare-workers": "^1.11.1",
    "@remix-run/node": "^1.14.3",
    "@remix-run/react": "^1.11.1",
    "@sereneinserenade/tiptap-extension-search-n-replace": "^2.0.0-beta.3",
    "@tiptap/core": "^2.0.2",
    "@tiptap/extension-bold": "^2.0.0-beta.209",
    "@tiptap/extension-collaboration": "^2.0.2",
    "@tiptap/extension-collaboration-cursor": "^2.0.2",
    "@tiptap/extension-document": "^2.0.0-beta.209",
    "@tiptap/extension-font-family": "^2.0.0-beta.220",
    "@tiptap/extension-hard-break": "^2.0.0-beta.219",
    "@tiptap/extension-highlight": "^2.0.0-beta.209",
    "@tiptap/extension-paragraph": "^2.0.0-beta.209",
    "@tiptap/extension-text": "^2.0.0-beta.209",
    "@tiptap/extension-text-style": "^2.0.0-beta.209",
    "@tiptap/pm": "^2.0.2",
    "@tiptap/react": "^2.0.0-beta.209",
    "@tiptap/starter-kit": "^2.0.2",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.1.1",
    "flowbite": "^1.6.3",
    "flowbite-react": "^0.3.8",
    "framer-motion": "^10.10.0",
    "prisma": "^4.12.0",
    "prosemirror-model": "^1.19.0",
    "prosemirror-state": "^1.4.2",
    "prosemirror-view": "^1.30.1",
    "react": "^18.2.0",
    "react-detect-click-outside": "^1.1.7",
    "react-dom": "^18.2.0",
    "react-responsive-modal": "^6.4.1",
    "react-spinners": "^0.13.8",
    "react-tailwindcss-datepicker": "^1.4.2",
    "recoil": "^0.7.7",
    "remix-utils": "^6.0.0",
    "tiptap-extension-font-size": "^1.2.0",
    "tiptap-extensions": "^1.35.2",
    "uuid": "^9.0.0",
    "y-prosemirror": "^1.2.1",
    "y-websocket": "^1.5.0",
    "yjs": "^13.5.52"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.19.0",
    "@remix-run/dev": "^1.11.1",
    "@remix-run/eslint-config": "^1.11.1",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "cypress": "^12.8.1",
    "eslint": "^8.27.0",
    "miniflare": "^2.11.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.4",
    "terser": "^5.16.6",
    "typescript": "^4.8.4",
    "wrangler": "^2.2.1"
  },
  "engines": {
    "node": ">=16.13"
  }
}
